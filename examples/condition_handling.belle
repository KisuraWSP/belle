declare bProc belle();
declare bVar x : bUinteger->69420;

implement{
	(* conditions always throw errors in this language if not handled then it will error *)
	declare bError{
		check(x>100);
	};
	if x != bError then{
		bPrint(x);
	}else{
		bPrint(bError.trace);
	};
		
}for belle();

(* for function calls *)
call belle(); 
